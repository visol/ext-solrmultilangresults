{
	"name": "visol/solrmultilangresults",
	"type": "typo3-cms-extension",
	"description": "Displays a hint about search results in other languages of a website",
	"homepage": "https://github.com/visol/ext-solrmultilangresults",
	"license": ["GPL-2.0-or-later"],

	"extra": {
		"typo3/cms": {
			"extension-key": "solrmultilangresults"
		}
	},
	"autoload": {
		"psr-4": {
			"Visol\\Solrmultilangresults\\": "Classes"
		}
	},
	"require": {
		"typo3/cms-core": "^11.5",
		"apache-solr-for-typo3/solr": "^11.5 || 11.5.x-dev"
	}
}
